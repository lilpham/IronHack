<h1> Editing Time Entries for <%= @project.name %> </h1>

<%= form_for [@project, @time_entry] do |f| %>
	<%= f.label :hours %>
	<%= f.text_field :hours %>
	<br>
	<%= f.label :minutes %>
	<%= f.text_field :minutes %>
	<br>
	<%= f.label :date %>
	<%= f.date_field :date %>
	<br>
	<%= f.button %>
<%end %>